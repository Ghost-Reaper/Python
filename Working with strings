phrase="GiraffeAcademy"


print(phrase)
#printinupercase
print(phrase.upper())
#printinlowercase
print(phrase.lower())
#cantellhowmanycharinastring
print(len(phrase))

#printingfirstcharacter
print(phrase[0])

#indexfunctionletusknowwherecharislocated
print(phrase.index("a"))

#replacefunction
print(phrase.replace("Giraffe","Elephant"))


#writingontwoseperatrlines
print("Giraffe\nAcademy")
